library(ggplot2)
library(dplyr)
library(tidyr)
data <- read.csv("maryland_crash_report.csv")
filtered_data <- data %>%
select(Collision.Type)
collision_types <- unique(filtered_data)
print(collision_types)
# Assuming 'data' is your dataset and 'collision_type_column' is the name of the column containing the collision types
collision_counts <- table(data$Collision.Type)
collision_counts_df <- as.data.frame(collision_counts)
colnames(collision_counts_df) <- c("Collision_Type", "Count")
# Create a bar plot
bar_plot <- ggplot(collision_counts_df, aes(x = Collision_Type, y = Count)) +
geom_bar(stat = "identity", fill = "steelblue") +
theme(axis.text.x = element_text(angle = 45, hjust = 1)) # To rotate x-axis labels for better readability
# Display the plot
print(bar_plot)
